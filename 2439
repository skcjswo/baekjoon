// 백준 2439

#include <iostream>
using namespace std;

int main()
{
    int n;
    cin >> n;
    
    for(int i = 1; i<= n; i++)
    {
        for(int j = 0; j < n - i; j++)// 공백 출력
        {
            cout << " ";
        }
        for(int j = 0; j < i; j++) 
        {
            cout << "*";
        }
         cout << endl;
    }
}
